{
  "name": "userscripts",
  "author": "jcwillox",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcwillox/userscripts"
  },
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "open": "ts-node --skipProject scripts/open-userscript.ts",
    "typecheck": "tsc",
    "lint": "eslint . --cache --max-warnings=0 --ext js,cjs,mjs,jsx,ts,tsx",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "update:readme": "md-template table -p userscripts"
  },
  "dependencies": {
    "@mdi/js": "7.2.96",
    "highlight.js": "11.8.0",
    "open": "9.1.0",
    "prettier": "2.8.8",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "18.16.12",
    "@types/tampermonkey": "4.0.10",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "rollup-plugin-userscript-metablock": "0.3.1",
    "typescript": "5.0.4",
    "vite": "4.3.7"
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": "^18"
  },
  "pnpm": {
    "patchedDependencies": {
      "rollup-plugin-userscript-metablock@0.3.1": "patches/rollup-plugin-userscript-metablock@0.3.1.patch"
    }
  }
}
